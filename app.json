{
  "expo": {
    "name": "eventsfinder",
    "slug": "eventsfinder",
    "version": "1.0.10",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arenamatteo.eventsfinder"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.matteo_arena.eventsfinder",
      "versionCode": 8
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3e6aa583-83f1-496f-88fe-456bb3ccc770"
      }
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
